{
  "name": "jekyll-react-boilerplate",
  "version": "1.0.0",
  "description": "Webpack, Jekyll, React",
  "keywords": [
    "jekyll",
    "webpack",
    "react"
  ],
  "bugs": {},
  "license": "MIT",
  "author": "Chris Schweda",
  "main": "bundle.js",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"webpack -w\" \"jekyll serve -w --incremental\"",
    "build": "webpack -p && jekyll build && npm run cleancss && npm run minifyhtml",
    "build-now": "webpack -p && jekyll build && npm run cleancss && npm run minifyhtml && now ./public",
    "build-serve": "webpack -p && jekyll build && npm run cleancss && npm run minifyhtml && serve ./public -o",
    "minifyhtml": "html-minifier --file-ext html --input-dir ./public --output-dir ./public",
    "cleancss": "cleancss -o  public/css/main.css public/css/main.css --source-map --debug --root ./"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clean-css": "^3.4.23",
    "concurrently": "^3.1.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-minifier": "^3.2.3",
    "http-server": "^0.9.0",
    "node-sass": "^4.1.1",
    "react": "^15.0.2",
    "react-addons-update": "^15.0.2",
    "react-dom": "^15.0.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "vue": "^2.1.8",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
