{
	"name": "jekyll-react-boilerplate",
	"version": "1.0.0",
	"description": "Webpack, Jekyll, React",
	"keywords": [
		"jekyll",
		"webpack",
		"react"
	],
	"bugs": {},
	"license": "MIT",
	"author": "Chris Schweda",
	"main": "bundle.js",
	"repository": {
		"type": "git"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "concurrently \"webpack -w\" \"jekyll serve -w --incremental\"",
		"build": "webpack -p && jekyll build && npm run cleancss",
		"build-now": "webpack -p && jekyll build && npm run cleancss & now ./public",
		"cleancss": "cleancss -o public/css/main.css public/css/main.css --source-map --debug"
	},
	"dependencies": {
		"concurrently": "^3.1.0"
	},
	"devDependencies": {
		"babel-core": "^6.7.7",
		"babel-loader": "^6.2.4",
		"babel-preset-es2015": "^6.6.0",
		"babel-preset-react": "^6.5.0",
		"clean-css": "^3.4.23",
		"react": "^15.0.2",
		"react-addons-update": "^15.0.2",
		"react-dom": "^15.0.2"
	}
}
