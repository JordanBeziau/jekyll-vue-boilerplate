{
	"name": "jekyll-vue-boilerplate",
	"version": "1.1.0",
	"description": "Webpack, Jekyll, Vue",
	"keywords": [
		"jekyll",
		"webpack",
		"react"
	],
	"bugs": {},
	"license": "MIT",
	"author": "Chris Schweda",
	"main": "bundle.js",
	"repository": {
		"type": "git"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "concurrently \"cross-env NODE_ENV=development webpack -w\" \"jekyll serve -w --incremental\"",
		"build": "cross-env NODE_ENV=production webpack --progress && jekyll build && npm run htmlclean",
		"build-now": "cross-env NODE_ENV=production webpack --progress && jekyll build && npm run htmlclean && now ./public",
		"build-serve": "cross-env NODE_ENV=production webpack --progress && jekyll build && npm run htmlclean && serve ./public -o",
		"cleancss": "cleancss -o  public/css/main.css public/css/main.css --source-map --debug --root ./",
		"htmlclean": "htmlclean public -o public"
	},
	"dependencies": {
		"bootstrap": "^4.0.0-alpha.6",
		"font-awesome": "^4.7.0",
		"jquery": "^3.1.1",
		"lodash": "^4.17.4",
		"style-loader": "^0.13.1",
		"tether": "^1.4.0",
		"url-loader": "^0.5.7"
	},
	"devDependencies": {
		"babel-core": "^6.7.7",
		"babel-loader": "^6.2.4",
		"babel-preset-es2015": "^6.6.0",
		"babel-preset-es2015-native-modules": "^6.9.4",
		"babel-preset-react": "^6.5.0",
		"clean-css": "^3.4.23",
		"concurrently": "^3.1.0",
		"cross-env": "^3.0.0",
		"css-loader": "^0.25.0",
		"expose-loader": "^0.7.1",
		"extract-text-webpack-plugin": "^2.0.0-beta.4",
		"file-loader": "^0.9.0",
		"html-minifier": "^3.2.3",
		"htmlclean": "^2.7.8",
		"http-server": "^0.9.0",
		"node-sass": "^4.1.1",
		"react": "^15.0.2",
		"react-addons-update": "^15.0.2",
		"react-dom": "^15.0.2",
		"rimraf": "^2.5.4",
		"sass-loader": "^4.1.1",
		"vue": "^2.1.8",
		"vue-loader": "^10.0.0",
		"vue-template-compiler": "^2.1.0",
		"webpack": "^2.1.0-beta.25",
		"webpack-dashboard": "^0.2.1",
		"webpack-dev-server": "^2.1.0-beta.9"
	}
}
